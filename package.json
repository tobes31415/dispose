{
  "name": "@tobes31415/dispose",
  "version": "1.0.0",
  "description": "Dispose objects using the decorator pattern",
  "main": "lib/dispose.js",
  "types": "lib/dispose.d.ts",
  "scripts": {
    "test": "npm run build:compile:quick && npx jest bin",
    "build": "npm run build:compile:full && npm run build:copy",
    "build:compile:quick": "tsc",
    "build:compile:full": "tsc --declaration",
    "build:copy": "cpx \"bin/**/!(*.test){*.js,*.d.ts}\" \"lib\"",
    "prettier": "prettier --write \"src/**/*.ts\""
  },
  "author": "Jake Tober",
  "license": "MIT",
  "devDependencies": {
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/preset-env": "7.8.7",
    "babel-jest": "25.1.0",
    "cpx": "1.5.0",
    "jest": "25.1.0",
    "prettier": "^1.19.1",
    "typescript": "3.8.3"
  },
  "dependencies": {}
}
